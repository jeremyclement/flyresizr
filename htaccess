# ------------------------------------------------------------------------------
# | Rewrite engine                                                             |
# ------------------------------------------------------------------------------

<IfModule mod_rewrite.c>
    RewriteEngine On
    
    RewriteRule ^(.+)-(\d*x\d*)\.(jpe?g|png|gif)$ php-flyresizr/index.php?name=$1&size=$2&ext=$3 [L]
</IfModule>
