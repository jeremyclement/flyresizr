# Copy following lines in your .htaccess file              
# -------------------------------------------

<IfModule mod_rewrite.c>
    RewriteEngine On
    
    # flyresizR rewrite rule
    RewriteRule ^(.+)-((\d+[*@]?x)|(x\d+)|(\d+x\d+[!^<>@]?))\.(jpe?g|png|gif)$ php-flyresizr/?name=$1&size=$2&ext=$6 [L]
    
</IfModule>
